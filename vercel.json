{
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    },
    "api/hello.py": {
      "maxDuration": 10
    },
    "api/django_test.py": {
      "maxDuration": 20
    },
    "api/minimal_django.py": {
      "maxDuration": 15
    },
    "api/payment_test.py": {
      "maxDuration": 15
    },
    "api/health.py": {
      "maxDuration": 15
    },
    "api/products.py": {
      "maxDuration": 20
    },
    "api/django_debug.py": {
      "maxDuration": 30
    },
    "api/django_app.py": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/docs/(.*)",
      "destination": "/api/django_app?path=/api/docs/$1"
    },
    {
      "source": "/api/schema/(.*)",
      "destination": "/api/django_app?path=/api/schema/$1"
    },
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/django_app?path=/api/v1/$1"
    },
    {
      "source": "/api/health/(.*)",
      "destination": "/api/django_app?path=/api/health/$1"
    }
  ]
}
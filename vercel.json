{
  "version": 2,
  "builds": [
    {
      "src": "api/hello.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/hello",
      "dest": "/api/hello.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "config.settings.vercel"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    },
    "api/hello.py": {
      "maxDuration": 10
    }
  }
}